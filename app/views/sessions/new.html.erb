<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <div class="login-submit">
    <%=f.submit "Login", class: "waves-effect waves-light btn"%> <%= button_to "Create Account", new_user_path, method: :get, class: "waves-effect waves-light btn right ", id: "create-account" %>

  </div>
<% end %>
