<div class="row">
<% @items.each do |item| %>
  <div class="col s4">
    <div class="card">
      <div class="card-image">
        <img src="<%= item.image_tag %>" />
      </div>
      <div class="card-content">
          <span class="card-title"><%= item.title %></span>
          <p>
            <%= item.price %>
          </p>
          <%= button_to "Add to Cart", carts_path(item_id: item.id) %>
      </div>
    </div>
  </div>
<% end %>
</div>
