<h1><%= current_user.username %>'s Orders</h1>

<% @orders.each do |order| %>
  <table>
    <tr>
      <th>Order Number</th>
      <th>Order Total</th>
    </tr>
    <tr>
      <td>#<%= link_to order.id, order_path(order) %></td>
      <td><%= order.total_price %></td>
    </tr>
  </table>

<% end %>
