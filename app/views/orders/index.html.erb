<h1><%= current_user.username %>'s Orders</h1>

<% @orders.each do |order| %>
  <table>
    <tr>
      <th>Order Number</th>
      <th></th>
      <th>Order Total</th>
    </tr>
    <tr>
      <td>#<%= order.id %>
      <td><%= link_to "View", order_path(order) %></td>
      <td><%= order.total_price %></td>
    </tr>
  </table>

<% end %>
